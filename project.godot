config_version=4

[application]

config/name="Spaceship Shooter RPG Deluxe"
run/main_scene="res://Game/MainScreens/Splash.tscn"
config/icon="res://icon.png"

[autoload]

Persistence="*res://Game/Autoload/Persistence.tscn"
DataVersionManager="*res://Game/Autoload/DataVersionManager.gd"
Main="*res://Game/Autoload/Main.gd"
Shop="*res://Game/Autoload/Shop.gd"
MusicManager="*res://Game/Autoload/MusicManager.tscn"
SoundManager="*res://Game/Autoload/SoundManager.tscn"

[display]

window/size/width=400
window/size/height=640
window/size/test_width=400
window/size/test_height=640
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "PersistenceNode", "RPGElements" )

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

